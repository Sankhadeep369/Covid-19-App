<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid_19_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <supports-screens
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.COVID_19_APP">
        <activity android:name=".dev_info"></activity>
        <activity android:name=".abt_us" />
        <activity android:name=".Case101" />
        <activity android:name=".Login" />
        <activity android:name=".item_select_rus" />
        <activity android:name=".item_select_fra" />
        <activity android:name=".item_select_deu" />
        <activity android:name=".item_select_brazil" />
        <activity android:name=".item_select_aus" />
        <activity android:name=".item_select_china" />
        <activity android:name=".item_select_japan" />
        <activity android:name=".item_select_uk" />
        <activity android:name=".item_select_usa" />
        <activity android:name=".item_select" />
        <activity android:name=".Vaccination" />
        <activity android:name=".Prevention" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>